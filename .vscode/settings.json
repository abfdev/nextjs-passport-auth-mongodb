{
	"WillLuke.nextjs.addTypesOnSave": true,
	"WillLuke.nextjs.hasPrompted": true,
	"cSpell.words": ["Everify", "resave", "semibold", "tailwindcss", "Toastify", "Uaaq"],
	"editor.formatOnType": false,
	"diffEditor.codeLens": true,
	"diffEditor.wordWrap": "on",
	"prettier.endOfLine": "auto",
	"prettier.printWidth": 120,
	"prettier.proseWrap": "preserve",
	"prettier.requireConfig": false,
	"namespaceResolver.sortOnSave": false,
	"prettier.jsxBracketSameLine": true,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"html.format.endWithNewline": true,
	"html.format.indentHandlebars": true,
	"html.format.indentInnerHtml": true,
	"html.format.wrapLineLength": 80,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"codesnap.backgroundColor": "",
	"codesnap.boxShadow": "rgba(0, 0, 0, 0.55) 3px 5px 5px",
	"codesnap.containerPadding": "1rem",
	"codesnap.showWindowTitle": false,
	"codesnap.transparentBackground": true
}
